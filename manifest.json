{
    "manifest_version": 2,
    "name": "一个秒表",
    "version": "1.0.0",
    "description": "我用chrome扩展制作了一个秒表",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "一个秒表",
        "default_popup": "popup.html"
    }
}